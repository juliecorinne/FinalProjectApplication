<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.test.models.StudentEntity" table="Student" schema="finalprojectapp">
        <id name="userName">
            <column name="userName" sql-type="varchar(40)" length="40"/>
        </id>
        <property name="firstName">
            <column name="firstName" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="lastName">
            <column name="lastName" sql-type="varchar(60)" length="60"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar(40)" length="40"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="className">
            <column name="className" sql-type="varchar(100)" length="100"/>
        </property>
        <property name="testResults">
            <column name="testResults" sql-type="varchar(45)" length="45" />
        </property>
        <property name="oppenness">
            <column name="oppenness" sql-type="double" precision="-1" />
        </property>
        <property name="emotionalRange">
            <column name="emotionalRange" sql-type="double" precision="-1" />
        </property>
        <property name="agreeableness">
            <column name="agreeableness" sql-type="double" precision="-1" />
        </property>
        <property name="introExtro">
            <column name="introExtro" sql-type="double" precision="-1" />
        </property>
        <property name="conscientiousness">
            <column name="conscientiousness" sql-type="double" precision="-1" />
        </property>
    </class>
</hibernate-mapping>